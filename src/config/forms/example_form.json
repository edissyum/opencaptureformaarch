{
    "FIELDS": {
        "CONTACT": {
            "table": "contacts",
            "data": {
                "lastname": {
                    "regex": "Nom\\s*:\\s*(.+)",
                    "column": "lastname"
                },
                "firstname": {
                    "regex": "Prenom\\s*:\\s*(.+)",
                    "column": "firstname"
                },
                "email": {
                    "regex": "Email\\s*:\\s*(.+)",
                    "column": "email"
                },
                "phone": {
                    "regex": "Telephone\\s*:\\s*(.+)",
                    "column": "phone"
                }
            }
        },
        "LETTERBOX": {
            "table": "res_letterbox",
            "data": [
                {
                    "regex": "Objet\\s*:\\s*(.+)",
                    "column": "subject"
                },
                {
                    "regex": "Précisions\\s*:\\s*(.+)",
                    "column": "custom",
                    "mapping": [
                        {
                            "isCustom": "True",
                            "isAddress": "False",
                            "column": "12"
                        }
                    ]
                },
                {
                    "regex": "Lieu d’intervention\\s*:\\s*(.+)",
                    "column": "custom",
                    "mapping": [
                        {
                            "isCustom": "True",
                            "isAddress": "True",
                            "column": "14"
                        },
                        {
                            "isCustom": "True",
                            "isAddress": "True",
                            "column": "14"
                        }
                    ]
                }
            ]
        }
    }
}